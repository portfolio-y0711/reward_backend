{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "rootDir": ".",
  "testMatch": [
    "<rootDir>./tests/_i11/_1_databases/**/*test.ts"
  ],
  "testEnvironment": "node",
  "transform": {
    "^.+\\.(t)s$": "ts-jest"
  },
  "moduleNameMapper": {
    "@app/(.*)$": "<rootDir>/src/$1",
    "@datasource/(.*)$": "<rootDir>/database/$1"
  },
  "setupFilesAfterEnv": ["<rootDir>/tests/config/setup-db.i11.ts"],
  "bail": 1
}
